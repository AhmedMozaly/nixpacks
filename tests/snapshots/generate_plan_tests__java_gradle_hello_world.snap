---
source: tests/generate_plan_tests.rs
expression: plan
---
{
  "nixpacksVersion": "[version]",
  "buildImage": "[build_image]",
  "variables": {
    "NIXPACKS_METADATA": "Java"
  },
  "phases": [
    {
      "name": "install",
      "dependsOn": [
        "setup"
      ]
    },
    {
      "name": "setup",
      "nixPackages": [
        {
          "name": "jdk"
        },
        {
          "name": "gradle"
        }
      ]
    },
    {
      "name": "build",
      "dependsOn": [
        "install"
      ],
      "commands": [
        "./gradlew build -x check"
      ],
      "cacheDirectories": [
        ".gradle"
      ]
    }
  ],
  "startPhase": {
    "cmd": "java $JAVA_OPTS -jar  build/libs/*.jar"
  }
}
